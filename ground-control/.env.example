# Source Registry
HARBOR_USERNAME=
HARBOR_PASSWORD=
HARBOR_URL=
# Ground Control PORT
PORT=9090

# Admin Authentication (username is always 'admin')
ADMIN_PASSWORD=

# Session Configuration
SESSION_DURATION_HOURS=24
LOCKOUT_DURATION_MINUTES=15

# Password Policy Configuration
PASSWORD_MIN_LENGTH=8
PASSWORD_MAX_LENGTH=128
PASSWORD_REQUIRE_UPPERCASE=true
PASSWORD_REQUIRE_LOWERCASE=true
PASSWORD_REQUIRE_NUMBER=true
PASSWORD_REQUIRE_SPECIAL=false

DB_HOST=
DB_PORT=
DB_DATABASE=
# Customize user and pass based on your config
DB_USERNAME=
DB_PASSWORD=
# Database TLS mode (disable, require, verify-ca, verify-full)
DB_SSLMODE=disable

# FIPS/Security Configuration
# Enable FIPS 140-2 compliant cryptography (requires DB_SSLMODE != disable and TLS cert/key)
FIPS_MODE=false
# TLS certificate and key paths (enables HTTPS when both set)
TLS_CERT_PATH=
TLS_KEY_PATH=
